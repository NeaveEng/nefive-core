<?xml version="1.0"?>
<!-- 
    Notes:
        Useful magic numbers:
            45                      0.785
            90degrees for RPY is    1.57

 -->
<robot name="nefive">

    <!-- #region - Materials -->
    <material name="red">
        <color rgba="0.8 0 0 1"/>
    </material>

    <material name="green">
        <color rgba="0 0.8 0 1"/>
    </material>

    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
