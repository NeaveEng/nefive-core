    <material name="white">
        <color rgba="0.8 0.8 0.8 1"/>
    </material>
    <!-- #endregion -->

    <!-- #region - Chassis-->
    <link name="base_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.105 0.146 0.068"/>
            </geometry>
            <material name="grey"/>
        </visual>
    </link>

    <!-- Drive sprockets -->
    <link name="roll_M0_link">
        <visual>
            <origin rpy="1.57 0 0" xyz="-0.074 0.086 -0.017"/>
            <geometry>
                <cylinder radius="0.023" length="0.022"/>
            </geometry>
            <material name="black"/>
        </visual>
    </link>
   
    <joint name="roll_joint_M0" type="continuous">
        <parent link="base_link"/>
        <child link="roll_M0_link"/>
        <origin xyz="0.0023 0 -0.0005" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
    </joint>

    <link name="roll_M1_link">
